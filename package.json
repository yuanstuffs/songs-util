{
  "name": "songs-util",
  "description": "Sync songs to another drive (/) or directory",
  "main": "dist/cli.js",
  "version": "1.1.0",
  "type": "module",
  "bin": {
    "songsutil": "./dist/cli.js"
  },
  "imports": {
    "#commands/*": "./dist/commands/*.js",
    "#utils/*": "./dist/lib/utils/*.js",
    "#lib/structures": "./dist/lib/structures/index.js",
    "#lib/*": "./dist/lib/*.js"
  },
  "scripts": {
    "build": "yarn clean && tsc -b src && yarn fixfile",
    "fixfile": "node scripts/executable.mjs",
    "clean": "rimraf dist",
    "watch": "yarn clean && tsc -b src -w",
    "lint": "eslint src --ext ts --fix",
    "format": "prettier src --write",
    "update": "yarn upgrade-interactive",
    "cli": "node --enable-source-maps dist/cli.js"
  },
  "devDependencies": {
    "@favware/colorette-spinner": "^1.0.1",
    "@sapphire/eslint-config": "^5.0.5",
    "@sapphire/result": "^2.7.2",
    "@sapphire/ts-config": "^5.0.1",
    "@skyra/env-utilities": "^1.3.0",
    "@types/node": "20.17.9",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.4.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@sapphire/pieces": "^4.3.1",
    "@sapphire/utilities": "^3.18.1",
    "colorette": "^2.0.20",
    "commander": "^12.1.0",
    "prompts": "^2.4.2"
  },
  "keywords": [
    "sync",
    "songs"
  ],
  "author": "yuanstuffs",
  "license": "Apache-2.0",
  "packageManager": "yarn@4.5.3"
}
